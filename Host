import java.util.*;
class Host {
private int hostId;
private String name;
private String contact;
public Host(int hostId, String name, String contact) {
this.hostId = hostId;
this.name = name;
this.contact = contact;
}
public int getHostId() { return hostId; }
public String getName() { return name; }
public String getContact() { return contact; }
@Override
public String toString() {
return "Host ID: " + hostId + ", Name: " + name + ", Contact: " + contact;
}
}
